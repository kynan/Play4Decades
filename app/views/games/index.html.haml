- title "Games"

%table
  %tr
    %th State
    %th Current round
    %th Nrounds
  - for @game in @games
    %tr
      %td= @game.state
      %td= @game.current_round
      %td= @game.nrounds
      %td
        = link_to 'Show', @game
        |
        = render 'games/links'

%p= link_to "New Game", new_game_path
